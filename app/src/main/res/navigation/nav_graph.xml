<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/fragment_home"
    tools:layout="@layout/activity_main">
    <fragment
        android:id="@+id/fragment_home"
        android:name="it.unimib.letsgreen.Home"
        android:label="Home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_fragment_home_to_mainActivity"
            app:destination="@id/mainActivity" />
        <action
            android:id="@+id/action_fragment_home_to_settings3"
            app:destination="@id/settings3" />
    </fragment>
    <fragment
        android:id="@+id/settings3"
        android:name="it.unimib.letsgreen.Settings"
        android:label="settings_fragment"
        tools:layout="@layout/settings_fragment" >
        <action
            android:id="@+id/action_settings3_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <fragment
        android:id="@+id/catalogo3"
        android:name="it.unimib.letsgreen.Catalogo"
        android:label="fragment_catalogo"
        tools:layout="@layout/fragment_catalogo" >
        <action
            android:id="@+id/action_catalogo3_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="it.unimib.letsgreen.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
</navigation>